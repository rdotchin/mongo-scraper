
<div class="container">
	{{!-- jumbotron --}}
	<div class="jumbotron">
	 	<h1>Mongo Scraper</h1>
	 	<p>New York Times Edition</p>
	</div>

	<div class="panel panel-primary">
        {{#each news}}
            <div class="panel-heading"><a target="_blank" href="{{this.link}}" class="title">{{this.headline}}</a></div>
  		    <div class="panel-body">
                {{this.summary}}
                <a href="/save/{{this._id}}"><button>Save</button></a>
            </div>


        {{/each}}



</div>{{!-- /container div --}}